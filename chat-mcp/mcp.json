{
    "servers": {
        "filesystem": {
            "type": "stdio",
            "command": "npx",
            "args": ["-y", "@modelcontextprotocol/server-filesystem", "/private/tmp"]
        },
        "brave-search": {
            "type": "stdio",
            "command": "npx",
            "args": ["-y", "brave-search-mcp"],
            "env": {
                "BRAVE_API_KEY": "${BRAVE_API_KEY}"
            }
        }
    }
}
